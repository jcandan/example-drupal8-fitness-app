{#
/**
 * @file exercise.html.twig
 * Default theme implementation to present Exercise data.
 *
 * This template is used when viewing Exercise pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_exercise()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('exercise') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
